  "scripts": {
    "build": "node src/generator.js",
    "watch": "nodemon --watch src --exec 'npm run build'",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Mexican Night",
        "uiTheme": "vs-dark",
        "path": "./themes/Mexican Night-color-theme.json"
      }
